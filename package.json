{
  "name": "nextjs-typescript-boilerplate",
  "version": "2.0.0",
  "description": "The boilerplate of nextjs with typescript.",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build && tsc --project tsconfig.json",
    "export": "next export",
    "deploy": "npm run build && npm run export",
    "production": "NODE_ENV=production",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": "https://github.com/zhangist/nextjs-typescript-boilerplate",
  "dependencies": {
    "express": "^4.17.1",
    "html-react-parser": "^0.8.1",
    "lodash": "^4.17.11",
    "next": "^9.0.0",
    "react": "^16.8.6",
    "react-autosize-textarea": "^7.0.0",
    "react-dom": "^16.8.6",
    "react-icons": "^3.7.0",
    "react-outside-click-handler": "^1.2.3",
    "styled-components": "^4.3.2",
    "styled-reset": "^2.0.16",
    "ts-jest": "^24.0.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.135",
    "@types/next": "8.0.5",
    "@types/react": "^16.8.21",
    "@types/react-dom": "16.8.4",
    "@types/react-html-parser": "^2.0.0",
    "@types/react-icons": "^3.0.0",
    "@types/react-outside-click-handler": "^1.2.0",
    "@types/react-test-renderer": "^16.8.2",
    "@types/styled-components": "^4.1.16",
    "jest": "^24.8.0",
    "react-router-dom": "^5.0.1",
    "react-test-renderer": "^16.8.6",
    "typescript": "3.5.2"
  }
